{
  "name": "koa-extern",
  "version": "0.1.1",
  "description": "extend for koa",
  "main": "./index.js",
  "scripts": {
    "test": "./node_modules/.bin/cross-env NODE_ENV=travis jest ./test/test.js --coverage",
    "cov": "./node_modules/.bin/cross-env NODE_ENV=travis ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./test/test.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "debug": "nodemon ./test/test.js"
  },
  "keywords": [
    "koa2",
    "extend",
    "xtx1130"
  ],
  "author": "xtx1130",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.2.0",
    "koa-router": "^7.2.0",
    "koa2-request-middleware": "^2.0.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "tough-cookie": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "istanbul": "^0.4.5",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "regenerator-runtime": "^0.10.5",
    "testing": "^1.1.1"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xtx1130/koa-extern"
  },
  "jest":{
    "coverageDirectory":"./coverage",
    "collectCoverage":true,
    "collectCoverageFrom":["!**/node_modules/**","!**/test/**","**/*.js","!**/coverage/**"]
  }
}
